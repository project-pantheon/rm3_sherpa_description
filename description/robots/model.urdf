    <parent link="rbsherpa_hl_base_link"/>
    <child link="rbsherpa_hl_torso_elevator_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="-0.36280 0.0090 0.8643"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000" lower="0.0" upper="5" velocity="0.5"/>
  </joint>
  <link name="rbsherpa_hl_torso_elevator_link">
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.51185"/>
      <geometry>
        <box size="0.04507 0.13725 1.0237"/>
      </geometry>
    </visual>
    <inertial>
