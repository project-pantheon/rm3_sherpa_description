<?xml version="1.0" ?>
<!-- Sherpa -->
<robot name="sherpa" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Base Link -->
  <link name="baselink">
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.001"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>

  </link>

  <!-- Chassis -->
  <joint name="rbsherpa_hl_base_footprint_joint" type="fixed">
    <parent link="baselink"/>
    <child link="rbsherpa_hl_base_link"/>
    <origin xyz="0.622 0.0 0.0"/>
  </joint>

  <link name="rbsherpa_hl_base_link">
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.6671"/>
      <geometry>
        <box size="1.6434 1.004 0.3944"/>
      </geometry>
    </visual>

    <collision>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.6671"/>
      <geometry>
        <box size="1.6434 1.004 0.3944"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.6671"/>
      <mass value="1"/>
      <inertia ixx="0.0969639466667" ixy="0.0" ixz="0.0" iyy="0.238026243333" iyz="0.0" izz="0.309064963333"/>
    </inertial>
  </link>
  <gazebo reference="rbsherpa_hl_base_link">
    <material>Gazebo/Red</material>
  </gazebo>


  <!-- ros_control plugin -->
  <gazebo>
    <static>false</static>
    <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
      <robotNamespace>/sherpa</robotNamespace>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
      <legacyModeNS>true</legacyModeNS>
    </plugin>
  </gazebo>

